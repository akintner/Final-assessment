<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  
  <%= csrf_meta_tags %>
</head>
<body>
  <% if page_is_login_or_create? %>
      <%= render partial: 'layouts/backdrop' %>
  <% end %>
  <div class="container">
    <main>
      <%= render partial: 'shared/errors' %>
      <%= render partial: 'shared/messages' %>
      <%= yield %>
    </main>
  </div>
</body>
</html>
